server {
    charset utf-8;
    client_max_body_size 128M;

    server_name smt-finder.local;
    listen 80;

    root {{ document_root }}/frontend/public;
    index index.html index.htm;

    error_log /var/log/nginx/frontend_error.log;
    access_log /var/log/nginx/frontend_access.log;

    location / {
    }
}
